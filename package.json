{
  "name": "huxy-admin",
  "version": "1.6.0",
  "type": "module",
  "description": "Customizable admin template for efficient and convenient development of maintainable and scalable systems, providing customized software design and development.",
  "keywords": [
    "react",
    "low-code",
    "web-design",
    "scene-design",
    "json schema",
    "useRouter",
    "useStore",
    "chatgpt",
    "huxy",
    "web3",
    "webgl",
    "hooks",
    "webpack",
    "full-stack",
    "MERN",
    "customizable",
    "responsive",
    "admin",
    "template"
  ],
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "postinstall": "husky install",
    "start": "pack start",
    "build": "pack run build",
    "analyze": "pack run analyze",
    "server": "pack run server",
    "test": "pack run test",
    "release": "pack run release",
    "eslint": "eslint 'app/**/*.{js,jsx}'",
    "stylelint": "stylelint 'app/**/*.{css,less}'",
    "lint": "npm run eslint && npm run stylelint",
    "lint-fix": "eslint --fix 'app/**/*.{js,jsx}' && stylelint --fix 'app/**/*.{css,less}'",
    "prettier": "prettier 'app/**/*' --write --ignore-unknown && npm run lint-fix",
    "server-fix": "eslint --fix 'server/**/*.{js,jsx,mjs}'",
    "server-pre": "prettier 'server/**/*' --write --ignore-unknown && npm run server-fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/huxy-admin"
  },
  "license": "MIT",
  "devDependencies": {
    "@huxy/pack": "^1.3.0",
    "husky": "^8.0.3"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@huxy/components": "^2.0.10",
    "@huxy/layout": "^2.0.8",
    "@huxy/materials": "^1.0.3",
    "@huxy/router": "^2.0.5",
    "@huxy/use": "^2.0.3",
    "@huxy/utils": "^2.0.11",
    "antd": "^5.6.4",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.9",
    "react-window-infinite-loader": "^1.0.9"
  }
}
