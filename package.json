{
  "name": "huxy-admin",
  "version": "1.4.0",
  "type": "module",
  "description": "Free admin dashboard template. Built with React18, Webpack5, useRouter, useStore, eslint, prettier, husky, commitlint, etc.",
  "keywords": [
    "React18",
    "useRouter",
    "useStore",
    "layout",
    "low-code",
    "no-code",
    "web-design",
    "scene-design",
    "web3",
    "webgl",
    "hooks",
    "Webpack5",
    "eslint",
    "prettier",
    "husky",
    "commitlint",
    "admin",
    "template",
    "dashboard"
  ],
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "postinstall": "husky install",
    "start": "pack start",
    "start1": "npm run start --dirname='app1'",
    "build": "pack run build",
    "analyze": "pack run analyze",
    "server": "pack run server",
    "test": "pack run test",
    "release": "pack run release",
    "eslint": "eslint 'app/**/*.{js,jsx}'",
    "stylelint": "stylelint 'app/**/*.{css,less}'",
    "lint": "npm run eslint && npm run stylelint",
    "lint-fix": "eslint --fix 'app/**/*.{js,jsx}' && stylelint --fix 'app/**/*.{css,less}'",
    "prettier": "prettier 'app/**/*' --write --ignore-unknown && npm run lint-fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/huxy-admin"
  },
  "license": "MIT",
  "devDependencies": {
    "@huxy/pack": "^1.0.6",
    "husky": "^8.0.3"
  },
  "dependencies": {
    "@huxy/components": "^2.0.2",
    "@huxy/layout": "^2.0.1",
    "@huxy/materials": "^1.0.1",
    "@huxy/router": "^2.0.1",
    "@huxy/use": "^2.0.1",
    "@huxy/utils": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
