{
  "name": "huxy-admin",
  "version": "1.5.0",
  "type": "module",
  "description": "Customizable admin dashboard template based on React18.",
  "keywords": [
    "react",
    "low-code",
    "web-design",
    "scene-design",
    "json schema",
    "useRouter",
    "useStore",
    "chatgpt",
    "huxy",
    "web3",
    "webgl",
    "hooks",
    "webpack",
    "eslint",
    "prettier",
    "husky",
    "commitlint",
    "admin",
    "template",
    "dashboard"
  ],
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "postinstall": "husky install",
    "start": "pack start",
    "start1": "npm run start --dirname='app1'",
    "build": "pack run build",
    "analyze": "pack run analyze",
    "server": "pack run server",
    "test": "pack run test",
    "release": "pack run release",
    "eslint": "eslint 'app/**/*.{js,jsx}'",
    "stylelint": "stylelint 'app/**/*.{css,less}'",
    "lint": "npm run eslint && npm run stylelint",
    "lint-fix": "eslint --fix 'app/**/*.{js,jsx}' && stylelint --fix 'app/**/*.{css,less}'",
    "prettier": "prettier 'app/**/*' --write --ignore-unknown && npm run lint-fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/huxy-admin"
  },
  "license": "MIT",
  "devDependencies": {
    "@huxy/pack": "^1.2.9",
    "husky": "^8.0.3"
  },
  "dependencies": {
    "@huxy/components": "^2.0.9",
    "@huxy/layout": "^2.0.8",
    "@huxy/materials": "^1.0.3",
    "@huxy/router": "^2.0.5",
    "@huxy/use": "^2.0.3",
    "@huxy/utils": "^2.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
